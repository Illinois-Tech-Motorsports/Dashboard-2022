<configuration debug="true">
    <appender name="DASH_APP" class="com.iit.dashboard2022.logging.StringAppender">
        <encoder>
            <pattern>[%d{HH:mm:ss.SSS} %level] [%logger{36}]: %message%n</pattern>
        </encoder>
    </appender>
    <root level="debug">
        <appender-ref ref="DASH_APP"/>
    </root>
</configuration>
